{
  "connection": "default",
  "collectionName": "encalendars",
  "info": {
    "name": "encalendar",
    "description": "Activities calendar for El Hotel in english"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "start": {
      "type": "date"
    },
    "photo": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    }
  }
}